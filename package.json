{
  "name": "@tweakly/multer-gcs-storage",
  "version": "0.9.0",
  "description": "A Google Cloud Storage (buckets) backend for multer",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "test": "c8 --100 ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tweakly/multer-gcs-storage.git"
  },
  "keywords": [
    "multer",
    "storage engine",
    "google cloud storage",
    "gcs",
    "gcp",
    "backend",
    "typescript"
  ],
  "author": "Lars Ivar Igesund",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tweakly/multer-gcs-storage/issues"
  },
  "homepage": "github.com/tweakly/multer-gcs-storage",
  "devDependencies": {
    "@septh/ts-run": "^1.2.4",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.4.1",
    "@types/urlencode": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "ava": "^6.1.3",
    "c8": "^10.1.2",
    "testcontainers": "^10.9.0",
    "tsup": "^8.2.4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.1",
    "urlencode": "^2.0.0",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "express": "^4.19.2",
    "multer": "^1.4.3"
  },
  "files": [
    "dist/**",
    "src/**",
    "tsconfig.json"
  ],
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "extensions": {
      "ts": "module",
      "mts": "module"
    },
    "nodeArguments": [
      "--import=@septh/ts-run/register",
      "--trace-deprecation"
    ]
  },
  "c8": {
    "exclude": [
      "test/**/*.ts",
      "src/gcp-specific.ts"
    ]
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
